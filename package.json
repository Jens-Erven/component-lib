{
  "name": "@jens_erven/brio-component-lib",
  "version": "0.0.1",
  "description": "A React component library for Brio with TypeScript, Material-UI, and Tailwind CSS",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run build:mui-themes && tsc && vite build",
    "preview": "vite preview",
    "prepublishOnly": "npm run build",
    "build:mui-themes": "node ./scripts/build-mui-themes.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "version:bump": "node ./scripts/version-bump.js",
    "publish:dry-run": "npm run build && npm publish --dry-run"
  },
  "keywords": [
    "react",
    "component-library",
    "typescript",
    "mui",
    "material-ui",
    "tailwind",
    "tailwindcss",
    "ui-components"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/component-library.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/component-library/issues"
  },
  "homepage": "https://github.com/your-username/component-library#readme",
  "peerDependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    },
    "@mui/material": {
      "optional": false
    },
    "@mui/icons-material": {
      "optional": false
    },
    "@emotion/react": {
      "optional": false
    },
    "@emotion/styled": {
      "optional": false
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "sideEffects": [
    "*.css",
    "dist/*.css"
  ],
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "@storybook/addon-a11y": "^10.1.9",
    "@storybook/addon-docs": "^10.1.9",
    "@storybook/addon-themes": "^10.1.9",
    "@storybook/addon-vitest": "^10.1.9",
    "@storybook/react-vite": "^10.1.9",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^25.0.2",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/browser-playwright": "^4.0.16",
    "@vitest/coverage-v8": "^4.0.16",
    "autoprefixer": "^10.4.23",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "storybook": "^10.1.9",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "@jens_erven/design-tokens-brio": "^0.0.2"
  }
}
